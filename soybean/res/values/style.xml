    <!-- 无标题，无背景透明的,非全屏窗口 （改）	by chan -->
    <style name="TranslucentUnfullwidthWinStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- 无背景透明的,全屏窗口 由下往上升起，上往下消失 	by chan  2014.8.22 -->
    <style name="TranslucentFullwidthWinStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/activity_down_to_up_Animation</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- 下拉背景透明弹出框 -->
    <style name="DropDownUnfullwidthWinStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- activity 由下往上弹出 -->
    <style name="activityAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/push_up_in</item>
        <item name="android:activityOpenExitAnimation">@anim/push_up_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_up_in</item>
        <item name="android:activityCloseExitAnimation">@anim/push_up_out</item>
    </style>

    <!-- activity 由下往上全屏弹 -->
    <style name="activity_down_to_up_Animation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/up_to_open</item>
        <item name="android:activityOpenExitAnimation">@anim/down_to_close</item>
        <item name="android:activityCloseEnterAnimation">@anim/up_to_open</item>
        <item name="android:activityCloseExitAnimation">@anim/down_to_close</item>
    </style>
    <!-- activity 由上往下弹出 -->
    <style name="activitydownAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/push_up_out</item>
        <item name="android:activityOpenExitAnimation">@anim/push_up_in</item>
        <item name="android:activityCloseEnterAnimation">@anim/push_up_out</item>
        <item name="android:activityCloseExitAnimation">@anim/push_up_in</item>
    </style>


